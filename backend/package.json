    "@types/jasmine": "^3.10.3",
    "@types/jsonfile": "^6.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "find": "^0.3.0",
    "fs-extra": "^10.0.1",
    "jasmine": "^4.0.2",
    "nodemon": "^2.0.15",
